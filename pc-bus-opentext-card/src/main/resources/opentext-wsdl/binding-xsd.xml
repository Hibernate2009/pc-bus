<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings version="2.1"
               jaxb:extensionBindingPrefixes="xjc"
               xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
               xmlns:xs="http://www.w3.org/2001/XMLSchema"
               wsdlLocation="26_27_28-ADService.wsdl">
    <jaxb:bindings schemaLocation="26_27_28-ADService.xsd" node=".">

        <jaxb:schemaBindings>
            <jaxb:package name="com.opentext.livelink.service.otintegration"/>
        </jaxb:schemaBindings>

        <jaxb:globalBindings mapSimpleTypeDef="false">
            <xjc:javaType name="org.joda.time.LocalDateTime" xmlType="xs:dateTime"
                          adapter="ru.alfastrah.interplat4.model.sed.JodaLocalDateTimeAdapter"/>
            <xjc:javaType name="org.joda.time.LocalDate" xmlType="xs:date"
                          adapter="ru.alfastrah.interplat4.model.sed.JodaLocalDateAdapter" />
        </jaxb:globalBindings>
    </jaxb:bindings>
</jaxb:bindings>